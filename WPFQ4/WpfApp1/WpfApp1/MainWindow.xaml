<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="ListBox1" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="360,78,104,133" IsEnabled="False" SelectionChanged="ListBox_SelectionChanged"/>
        <Button Content="Load CSV" HorizontalAlignment="Left" Margin="221,161,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <StatusBar Margin="0,389,0,0">
            <TextBlock x:Name="statusBarText"/>
        </StatusBar>

    </Grid>
</Window>
